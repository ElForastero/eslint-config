{
  "name": "@spaceship/eslint-config",
  "version": "0.2.3",
  "description": "Shareable ESLint configs for React, React Native, React Native Web, and Node projects.",
  "keywords": [
    "eslint",
    "eslint-config",
    "react",
    "react-native",
    "react-native-web"
  ],
  "files": [
    "README.md",
    "CHANGELOG.md",
    "hybrid.js",
    "index.js",
    "jest.js",
    "native.js",
    "node.js",
    "web.js",
    "i18n.js",
    "shared"
  ],
  "main": "index.js",
  "repository": "git://github.com/elforastero/eslint-config.git",
  "author": "Eugene Dzhumak",
  "license": "MIT",
  "private": false,
  "scripts": {
    "release": "standard-version"
  },
  "dependencies": {
    "@react-native-community/eslint-config": ">= 2.0.0",
    "@react-native-community/eslint-plugin": ">= 1.1.0",
    "@typescript-eslint/eslint-plugin": ">= 4.8.2",
    "@typescript-eslint/parser": ">= 4.9.0",
    "confusing-browser-globals": ">= 1.0.10",
    "eslint": ">= 7.14.0",
    "eslint-config-prettier": ">= 8.0.0",
    "eslint-plugin-eslint-comments": ">= 3.2.0",
    "eslint-plugin-i18next": ">= 5.1.1",
    "eslint-plugin-import": ">= 2.22.1",
    "eslint-plugin-prettier": ">= 4.0.0",
    "eslint-plugin-react": ">= 7.21.5",
    "eslint-plugin-react-native": ">= 3.10.0",
    "eslint-plugin-react-native-a11y": ">= 2.0.3",
    "eslint-plugin-testing-library": ">= 3.10.1",
    "prettier": ">= 2.2.1",
    "typescript": ">= 3"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "standard-version": "^9.1.1"
  },
  "lint-staged": {
    "*.js": "prettier --write"
  }
}
