{
  "name": "@spaceship/eslint-config",
  "version": "0.0.1",
  "description": "Shareable ESLint configs for React, React Native, React Native Web, and Node projects.",
  "keywords": [
    "eslint",
    "eslint-config",
    "react",
    "react-native",
    "react-native-web"
  ],
  "files": [
    "README.md",
    "CHANGELOG.md",
    "hybrid.js",
    "index.js",
    "jest.js",
    "native.js",
    "node.js",
    "web.js",
    "shared"
  ],
  "main": "index.js",
  "repository": "git://github.com/elforastero/eslint-config.git",
  "author": "Eugene Dzhumak",
  "license": "MIT",
  "private": false,
  "scripts": {
    "release": "standard-version"
  },
  "peerDependencies": {
    "@react-native-community/eslint-plugin": ">= 1.1.0",
    "@typescript-eslint/eslint-plugin": ">= 4.8.2",
    "eslint": ">= 7.14.0",
    "eslint-plugin-eslint-comments": ">= 3.2.0",
    "eslint-plugin-prettier": ">= 3.1.4",
    "eslint-plugin-react": ">= 7.21.5",
    "eslint-plugin-react-native": ">= 3.10.0",
    "eslint-plugin-react-native-a11y": ">= 2.0.3",
    "eslint-plugin-testing-library": ">= 3.10.1",
    "prettier": ">= 2.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "husky": "^4.3.0",
    "prettier": "^2.2.1",
    "standard-version": "^9.0.0"
  },
  "dependencies": {
    "confusing-browser-globals": "^1.0.10"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
